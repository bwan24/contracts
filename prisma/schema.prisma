generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Contract {
  id               String   @id @default(cuid())
  name             String
  type             String
  standard         String
  uploadDate       DateTime
  fileUrl          String
  fileSize         String
  status           String
  riskCount        Json
  createdAt        DateTime @default(now())
  updatedAt        DateTime @updatedAt
  fileContent      Bytes?
  fileType         String?
  markdown_content String?
}
